@model IEnumerable<UsersAdminPanel.Models.User>

@section styles
{
}

@{
    var grid = new WebGrid(source: Model, rowsPerPage: 5, ajaxUpdateContainerId: "grid");
}

@grid.GetHtml(
              htmlAttributes: new { cellpadding = "6px", cellspacing = "0px", id = "grid" },
              tableStyle: "grid",
              headerStyle: "header",
              alternatingRowStyle: "alt-row",
              rowStyle: "row",
              footerStyle: "footer",
              columns: grid.Columns(
                  grid.Column(columnName: "Name", header: "User Name")))
