@model IEnumerable<UsersAdminPanel.Models.User>

@section styles
{
    <link href="~/Content/GridStyles.css" rel="stylesheet" />
}

@{
    var grid = new WebGrid(source: Model, rowsPerPage: 5);
}

@grid.GetHtml(
              htmlAttributes: new { cellpadding = "6px", cellspacing = "0px" },
              columns: grid.Columns(
                  grid.Column(columnName: "Name", header: "User Name")))